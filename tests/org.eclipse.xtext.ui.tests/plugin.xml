<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="testExtensionPoint" name="Test Extension Point" schema="schema/testExtensionPoint.exsd"/>
   <extension
         point="org.eclipse.xtext.ui.languageDescriptor">
      <languageDescriptor
            id="org.eclipse.xtext.ui.tests.dummylanguage"
            name="Dummy Language"
            superLanguage="org.eclipse.xtext.builtin.XtextBuiltin">
      </languageDescriptor>
   </extension>
   <extension
         point="org.eclipse.xtext.ui.syntaxColorer">
      <syntaxColorer
            class="org.eclipse.xtext.ui.TestSyntaxColorer"
            languageId="org.eclipse.xtext.ui.tests.dummylanguage">
      </syntaxColorer>
   </extension>
   <extension
         point="org.eclipse.xtext.ui.tokenTypeDef">
      <tokenTypeDef
            class="org.eclipse.xtext.ui.TestTokenTypeDef"
            languageId="org.eclipse.xtext.ui.tests.dummylanguage">
      </tokenTypeDef>
   </extension>
   <extension
         point="org.eclipse.xtext.ui.tests.testExtensionPoint" id="myid">
      <test
            class="org.eclipse.xtext.ui.service.utils.XtextServiceExtensionFactory:org.eclipse.xtext.ui.services.bearing.SampleServiceDependentClass/org.eclipse.xtext.reference.ReferenceGrammar">
      </test>
   </extension>
</plugin>
