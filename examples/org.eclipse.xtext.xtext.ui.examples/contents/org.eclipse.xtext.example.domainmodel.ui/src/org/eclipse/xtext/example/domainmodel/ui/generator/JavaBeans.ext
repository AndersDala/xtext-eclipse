import org::eclipse::xtext::example::domainmodel::domainmodel;

extension org::eclipse::xtext::example::domainmodel::ui::generator::JavaImports;

packageName(Entity this) :
	qualifiedName(eContainer());
	
String qualifiedName(Void this) : null;
String qualifiedName(Entity this) : packageName()==null? name : packageName()+"."+name;
String qualifiedName(Object this) : null;
String qualifiedName(PackageDeclaration this) :
	if qualifiedName(eContainer())==null 
	then name
	else qualifiedName(eContainer())+'.'+name;
	
simpleName(Entity this) :
	registerImport(this.qualifiedName()) ->
	this.name;
   