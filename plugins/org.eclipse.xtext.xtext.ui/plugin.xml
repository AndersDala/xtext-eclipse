<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="Xtext"
            id="org.eclipse.xtext.projectwiz">
      </category>
      <wizard
            category="org.eclipse.xtext.projectwiz"
            class="org.eclipse.xtext.xtext.ui.wizard.project.NewXtextProjectWizard"
            icon="icons/editor.gif"
            id="org.eclipse.xtext.xtext.ui.wizard.project.NewXtextProjectWizard"
            name="New Xtext Project"
            project="true">
      </wizard>
   </extension>
   
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.eclipse.xtext.ui.core.editor.XtextEditor"
            contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
            default="true"
            extensions="xtext,xtext2"
            id="org.eclipse.xtext.Xtext"
            name="Xtext Editor">
      </editor>
   </extension>
 <extension
       point="org.eclipse.xtext.service.ui.serviceRegistrationFactory">
    <serviceRegistrationFactory
          class="org.eclipse.xtext.XtextUiConfig"/>
 </extension>
 <extension
         point="org.eclipse.ui.preferencePages">
		<page
			class="org.eclipse.xtext.service.ui.ServiceInjectingExecutableExtensionFactory:org.eclipse.xtext.ui.core.editor.preferences.LanguageRootPreferencePage/org.eclipse.xtext.Xtext"
			id="org.eclipse.xtext.Xtext"
			name="Xtext">
			<keywordReference
				id="org.eclipse.xtext.xtext.ui.keyword_Xtext">
			</keywordReference>
		</page>
		<page
		    category="org.eclipse.xtext.Xtext"
		    class="org.eclipse.xtext.service.ui.ServiceInjectingExecutableExtensionFactory:org.eclipse.xtext.ui.core.editor.preferences.EditorPreferencePage/org.eclipse.xtext.Xtext"
		    id="org.eclipse.xtext.Xtext.editor"
		    name="Editor">
		    	<keywordReference 
           			id="org.eclipse.xtext.keyword.editor">
     			</keywordReference>
		</page>
      </extension>
	<extension
		point="org.eclipse.ui.keywords">
		<keyword
			id="org.eclipse.xtext.xtext.ui.keyword_Xtext"
			label="Xtext">
		</keyword>
		<keyword
            id="org.eclipse.xtext.Xtext.tasktags"
            label="Task Tags">
        </keyword>
	</extension>

</plugin>
