<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="syntaxColorer" name="Xtext Syntax Colorer Service" schema="schema/syntaxColorer.exsd"/>
   <extension-point id="proposalsProvider" name="Xtext Proposals Provider Service" schema="schema/proposalsProvider.exsd"/>
   <extension-point id="preferenceStore" name="Xtext Preference Store Service" schema="schema/preferenceStore.exsd"/>
   <extension-point id="parser" name="parser" schema="schema/parser.exsd"/>
   <extension-point id="aSTFactory" name="Xtext AST Factory Service" schema="schema/aSTFactory.exsd"/>
   <extension-point id="parseTreeConstructor" name="Xtext Parse Tree Constructor Service" schema="schema/parseTreeConstructor.exsd"/>
   <extension-point id="resourceFactory" name="Xtext Resource Factory Service" schema="schema/resourceFactory.exsd"/>
   <extension-point id="grammarAccess" name="Xtext Grammar Access Service" schema="schema/grammarAccess.exsd"/>
   <extension-point id="metamodelAccess" name="Xtext Metamodel Access Service" schema="schema/metamodelAccess.exsd"/>
   <extension-point id="tokenTypeDef" name="Xtext Token Type Definition Service" schema="schema/tokenTypeDef.exsd"/>
   <extension-point id="valueConverter" name="Xtext Value Converter Service" schema="schema/valueConverter.exsd"/>
   <extension-point id="outlineProvider" name="Xtext Outline Content Provider" schema="schema/outlineProvider.exsd"/>
   <extension-point id="labelProvider" name="Xtext Label Provider" schema="schema/labelProvider.exsd"/>
   <extension-point id="formatter" name="Xtext Formatter" schema="schema/formatter.exsd"/>
   <extension-point id="hoverInfo" name="Xtext Hover Info" schema="schema/hoverInfo.exsd"/>
   <extension-point id="foldingStructureProvider" name="Xtext Folding Structure Provider" schema="schema/foldingStructureProvider.exsd"/>
   <extension
         id="org.eclipse.xtext.ui.problemmarker"
         name="Xtext Editor Problem Marker"
         point="org.eclipse.core.resources.markers">
      <super type="org.eclipse.emf.ecore.diagnostic"/>
      <persistent value="true"/>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
		<key sequence="M1+M2+F"
            contextId="org.eclipse.ui.textEditorScope"
            commandId="org.eclipse.xtext.ui.FormatAction"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
             </extension>
     <extension
         point="org.eclipse.ui.commands">  
             <command
            name="Format"
            description="Format text"
            categoryId="org.eclipse.ui.category.textEditor"
            id="org.eclipse.xtext.ui.FormatAction">
      </command>
    </extension>
   <extension
         point="org.eclipse.xtext.service.ui.scope">
      <serviceScope
            id="org.eclipse.xtext.builtin.XtextBuiltin">
      </serviceScope>
   </extension>
   <extension
         point="org.eclipse.xtext.ui.aSTFactory">
      <aSTFactory
            class="org.eclipse.xtext.parser.GenericEcoreElementFactory"
            languageId="org.eclipse.xtext.builtin.XtextBuiltin">
      </aSTFactory>
   </extension>
   <extension
         point="org.eclipse.xtext.ui.grammarAccess">
      <grammarAccess
            class="org.eclipse.xtext.builtin.XtextBuiltinGrammarAccess"
            languageId="org.eclipse.xtext.builtin.XtextBuiltin">
      </grammarAccess>
   </extension>
   <extension
         point="org.eclipse.xtext.ui.valueConverter">
      <valueConverter
            class="org.eclipse.xtext.builtin.conversion.XtextBuiltInConverters"
            languageId="org.eclipse.xtext.builtin.XtextBuiltin">
      </valueConverter>
   </extension>
   <extension
         point="org.eclipse.xtext.ui.syntaxColorer">
      <syntaxColorer
            class="org.eclipse.xtext.ui.service.impl.BuiltInSyntaxColorer"
            languageId="org.eclipse.xtext.builtin.XtextBuiltin">
      </syntaxColorer>
   </extension>
   <extension
         point="org.eclipse.xtext.ui.tokenTypeDef">
      <tokenTypeDef
            class="org.eclipse.xtext.ui.service.impl.BuiltInTokenTypeDef"
            languageId="org.eclipse.xtext.builtin.XtextBuiltin">
      </tokenTypeDef>
   </extension>
   <extension
         point="org.eclipse.xtext.ui.outlineProvider">
      <outlineProvider
            class="org.eclipse.xtext.ui.service.impl.BuiltInOutlineProvider"
            languageId="org.eclipse.xtext.builtin.XtextBuiltin">
      </outlineProvider>
   </extension>
   <extension
         point="org.eclipse.xtext.ui.labelProvider">
      <labelProvider
            class="org.eclipse.xtext.ui.service.impl.BuiltInLabelProvider"
            languageId="org.eclipse.xtext.builtin.XtextBuiltin">
      </labelProvider>
   </extension>
   <extension
         point="org.eclipse.xtext.ui.proposalsProvider">
      <proposalsProvider
            class="org.eclipse.xtext.ui.service.impl.BuiltInProposalsProvider"
            languageId="org.eclipse.xtext.builtin.XtextBuiltin">
      </proposalsProvider>
   </extension>
   <extension
         point="org.eclipse.xtext.ui.formatter">
      <formatter
            class="org.eclipse.xtext.ui.service.impl.BuiltInFormatter"
            languageId="org.eclipse.xtext.builtin.XtextBuiltin">
      </formatter>
   </extension>
   <extension
         point="org.eclipse.xtext.ui.hoverInfo">
      <hoverInfo
            class="org.eclipse.xtext.ui.service.impl.BuiltInHoverInfo"
            languageId="org.eclipse.xtext.builtin.XtextBuiltin">
      </hoverInfo>
   </extension>
   <extension
         point="org.eclipse.xtext.ui.foldingStructureProvider">
      <foldingStructureProvider
            class="org.eclipse.xtext.ui.service.impl.BuiltInFoldingStructureProvider"
            languageId="org.eclipse.xtext.builtin.XtextBuiltin">
      </foldingStructureProvider>
   </extension>
   <extension
         point="org.eclipse.xtext.service.ui.serviceRegistrationFactory">
      <serviceRegistrationFactory
            class="org.eclipse.xtext.ui.service.utils.XtextServiceRegistrationFactory">
      </serviceRegistrationFactory>
   </extension>
   <extension
         point="org.eclipse.ui.keywords">
      <keyword
            id="org.eclipse.xtext.keyword.editor"
            label="Editor">
      </keyword>
      <keyword
            id="org.eclipse.xtext.keyword.syntax_coloring"
            label="Syntax">
      </keyword>
   </extension>
   <extension
         point="org.eclipse.ui.ide.markerResolution">
      <markerResolutionGenerator
            class="org.eclipse.xtext.ui.editor.quickfix.XtextMarkerResolutionGenerator"
            markerType="org.eclipse.xtext.ui.problemmarker">
      </markerResolutionGenerator>
      <!--<markerResolutionGenerator
            class="org.eclipse.xtext.ui.editor.utils.XtextMarkerResolutionGenerator"
            markerType="org.eclipse.emf.ecore.diagnostic">
      </markerResolutionGenerator>-->
   </extension>
   
</plugin>
