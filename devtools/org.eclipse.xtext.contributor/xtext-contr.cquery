<?xml version="1.0" encoding="UTF-8"?>
<cq:componentQuery xmlns:cq="http://www.eclipse.org/buckminster/CQuery-1.0" resourceMap="xtext-contr.rmap">
    <cq:rootRequest name="org.eclipse.xtext.contributor" componentType="buckminster"/>
 	<cq:advisorNode namePattern="^.*\.source$" skipComponent="true"/>
     <cq:advisorNode namePattern="^org\.eclipse.*examples.*$" skipComponent="true"/>
     <cq:advisorNode namePattern="org.eclipse.xtend.typesystem" skipComponent="true"/>
     <cq:advisorNode namePattern="^(org|com|javax)\.([a-z])+(\.(?!(xtext|xtend\.|xtend2)).*)?$" skipComponent="true"/>
     <cq:advisorNode namePattern="^org\.junit(?:\..+)?$" skipComponent="true"/>
</cq:componentQuery>
